#!/usr/bin/python
#-*- coding=utf-8 -*-
sf = 0
for i in range(0, 5):
    af = range(5)
    af[i] = (int(raw_input('Pls input an integer:')))
    sf += af[i]
print "The sum is:",
print sf

sw = 0
j = 0
aw=range(5)
while j < len(aw):
    aw[j] = (int(raw_input('Pls input an integer:')))
    sw += aw[j]
    j += 1
print "The sum is:",
print sw
